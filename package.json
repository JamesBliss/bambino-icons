  "homepage": "https://jamesbliss.github.io/bambino-form",
  "scripts": {
    "storybook": "start-storybook -p 6007",
    "build-storybook": "build-storybook -s ./public -o .out",
    "deploy-storybook": "storybook-to-ghpages",
    "clean": "rimraf lib",
    "test": "jest --passWithNoTests",
    "test:watch": "yarn jest src --watch --notify",
    "cover": "jest src --coverage",
    "lint": "eslint src",
    "compile": "rm -rf dist/ && babel --copy-files src --out-dir dist",
    "build": "yarn clean && yarn lint && yarn test && yarn compile",
    "release": "chmod +x release.sh && ./release.sh && npm run deploy-storybook"
  },
  "peerDependencies": {
    "react": ">= 16.8",
    "react-dom": ">= 16.8",
    "styled-components": ">= 4"
  },
