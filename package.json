{
  "name": "bambino-form",
  "version": "0.0.0-beta.0",
  "description": "Il mio piccolo bambino, form.",
  "private": false,
  "license": "MIT",
  "main": "index.js",
  "codename": "quote-truth",
  "repository": {
    "type": "git",
    "url": "https://github.com/JamesBliss/bambino-form.git"
  },
  "author": "jamesbliss",
  "bugs": {
