{
  "extends": [
    "airbnb-base",
    "eslint-config-airbnb",
    "plugin:jest/recommended"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "jest"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "destructuring": true,
      "classes": true,
      "forOf": true,
      "blockBindings": true,
      "arrowFunctions": true
    }
  },
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["__get__"] }],
    "jsx-a11y/img-has-alt": "off",
    "max-len": 0,
    "arrow-body-style": 0,
    "arrow-parens": 0,
    "class-methods-use-this": 0,
    "func-names": 0,
    "indent": 2,
    "new-cap": 0,
    "no-plusplus": 0,
    "no-return-assign": 0,
    "quote-props": 0,
    "template-curly-spacing": [
      2,
      "always"
    ],
    "react/jsx-one-expression-per-line": 0,
    "no-trailing-spaces": [
      "error",
      {
        "skipBlankLines": true
      }
    ],
    "comma-dangle": [
      "error",
      "never"
    ],
    "jsx-quotes": [
      2,
      "prefer-single"
    ],
    "eol-last": [
      "error",
      "never"
    ],
    "react/forbid-prop-types": 0,
    "react/jsx-curly-spacing": [
      2,
      {"when": "always", "attributes": {"allowMultiline": false}, "children": true}
    ],
    "object-curly-newline": ["error", { "multiline": true, "minProperties": 10 }],
    "react/jsx-filename-extension": 0,
    "react/jsx-boolean-value": 0,
    "react/prefer-stateless-function": 0,
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "import/prefer-default-export": 0,
    "jsx-a11y/href-no-hash": "off",
    "jsx-a11y/anchor-is-valid": ["warn", { "aspects": ["invalidHref"] }]
  }
}